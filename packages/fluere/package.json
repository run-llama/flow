{
  "name": "@llamaindex/flow",
  "version": "0.2.0",
  "description": "event-based, stream oriented workflow engine",
  "keywords": [
    "workflow",
    "event",
    "stream",
    "rxjs",
    "event-driven",
    "llamaindex",
    "flow"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "workerd": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "edge-light": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.serverless.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./interrupter/*": {
      "types": "./interrupter/*.d.ts",
      "default": "./interrupter/*.js"
    },
    "./middleware/*": {
      "types": "./middleware/*.d.ts",
      "default": "./middleware/*.js"
    },
    "./util/*": {
      "types": "./util/*.d.ts",
      "default": "./util/*.js"
    },
    "./stream": {
      "types": "./stream/index.d.ts",
      "default": "./stream/index.js"
    }
  },
  "files": [
    "dist",
    "interrupter",
    "util",
    "middleware",
    "stream"
  ],
  "scripts": {
    "build": "rimraf dist interrupter middleware stream util && tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "prepublish": "cp ../../README.md ./README.md"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "hono": "^4.7.4",
    "next": "^15.2.2",
    "p-retry": "^6.2.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8",
    "zod": "^3.24.2"
  },
  "peerDependencies": {
    "hono": "^4.7.4",
    "next": "^15.2.2",
    "p-retry": "^6.2.1",
    "zod": "^3.24.2"
  },
  "license": "MIT",
  "peerDependenciesMeta": {
    "hono": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "p-retry": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  }
}
